{
  "sourceType": "module",
  "presets": [
    ["@babel/env", { "targets": { "node": "current" } }],
    ["@babel/preset-flow", { "all": true }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["transform-es2015-modules-commonjs", { "strictMode": false }],
    "syntax-flow"
  ]
}
